# Docker Compose Override for Development
# Copy this file to docker-compose.override.yml and update with your values

version: "3.8"

services:
  airflow-webserver:
    environment:
      # Replace with your actual SonarQube token
      AIRFLOW_VAR_SONARQUBE_TOKEN: 'your_actual_sonarqube_token_here'
  
  airflow-scheduler:
    environment:
      # Replace with your actual SonarQube token
      AIRFLOW_VAR_SONARQUBE_TOKEN: 'your_actual_sonarqube_token_here'